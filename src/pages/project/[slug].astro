---
import BaseLayout from "../../layouts/BaseLayout.astro";
export async function getStaticPaths() {
    const allPorjects:any = await Astro.glob('../../../src/content/project/*.mdx');
    return allPorjects.map((item) => ({
        params: { slug: item.url.split("/").pop() },
        props: {
            ...item,
        },
    }));
}
const {Content,frontmatter: { title },} = Astro.props;

---
<BaseLayout>
    <Content></Content>
</BaseLayout>